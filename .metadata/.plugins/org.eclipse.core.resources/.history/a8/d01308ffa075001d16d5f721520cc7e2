package ClasesOperadores;

public class Ejercicio2 {

	public static void main(String[] args) {
		int num1 = 9;
		int num2 = 7;
		float div = num1/num2
		
		System.out.println("La suma de los dos valores es: "+(num1+num2));
		System.out.println("La resta de los dos valores es: "+(num1-num2));
		System.out.println("La multiplicación de los dos valores es: "+(num1*num2));
		System.out.println("La división de los dos valores es: "+div);

	}

}
