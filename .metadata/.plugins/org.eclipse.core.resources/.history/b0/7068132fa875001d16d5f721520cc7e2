package ClaseMath;

public class Ejercicio2 {

	public static void main(String[] args) {
		System.out.println("Las raiz de 2,8,9,27,28,55,121 son: "+(Math.sqrt(2))+(Math.sqrt(8))+(Math.sqrt(9))+(Math.sqrt(27))+(Math.sqrt(28))+(Math.sqrt(55))+(Math.sqrt(121)));
		System.out.println("Las raiz de 8"+Math.sqrt(8));
	}

}
